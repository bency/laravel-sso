{
    "name": "bency/laravel-sso",
    "description": "Laravel SSO Solution",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Casper Lai",
            "email": "casper.lai@sleepingdesign.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bency/sso"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "bency/sso": "^1.0",
        "spatie/url": "^1.3",
        "illuminate/support": "^5.8|^6|^7|^8",
        "illuminate/database": "^5.8|^6|^7|^8",
        "illuminate/http": "^5.8|^6|^7|^8",
        "eastwest/json": "^3.0",
        "flugger/laravel-responder": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Casperlaitw\\LaravelSSO\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Casperlaitw\\LaravelSSO\\LaravelSSOProvider"
            ]
        }
    }
}
